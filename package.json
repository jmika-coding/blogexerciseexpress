{
  "name": "blogexerciseexpress",
  "version": "1.0.0",
  "description": "Blog exercise in Express typera typescript",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "tsnd --respawn --transpile-only --watch src,db,swagger,test src/index.ts",
    "check-lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "clean": "rm -r dist/",
    "generate-openapi": "npx typera-openapi src/index.ts"
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "eslint --fix --fix-type suggestion",
      "prettier --write"
    ],
    "*.{css,json}": [
      "prettier --write"
    ],
    "*.js": "eslint --cache --fix"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "printWidth": 100
  },
  "dependencies": {
    "express": "4.17.1",
    "node": "14.17.3",
    "typescript": "4.3.5",
    "typera-express": "2.4.1",
    "pg": "8.6.0",
    "postgres-migrations": "5.3.0",
    "dotenv": "10.0.0"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/node": "16.4.3",
    "@types/pg": "8.6.1",
    "prettier": "2.3.1",
    "husky": "7.0.1",
    "lint-staged": "11.1.1",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "ts-node-dev": "1.1.8",
    "typera-openapi": "1.0.3",
    "swagger-ui-express": "4.1.6",
    "@types/swagger-ui-express": "4.1.3"
  }
}
